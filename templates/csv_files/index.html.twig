{% extends 'base.html.twig' %}

{% block title %}CsvFiles index{% endblock %}

{% block body %}
    <h1>CsvFiles index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Url</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for csv_file in csv_files %}
            <tr>
                <td>{{ csv_file.id }}</td>
                <td>{{ csv_file.url }}</td>
                <td>
                    <a href="{{ path('app_csv_files_show', {'id': csv_file.id}) }}">show</a>
                    <a href="{{ path('app_csv_files_edit', {'id': csv_file.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_csv_files_new') }}">Create new</a>
{% endblock %}
